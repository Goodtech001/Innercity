{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "next/dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "page.tsx"
    },
    {
      "src": "/(.*)",
      "dest": "page.tsx"
    }
  ]
}